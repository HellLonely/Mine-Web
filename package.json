{
  "name": "web-mine",
  "version": "1.5.1",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "electron .",
    "build": "electron-builder",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.myapp",
    "productName": "Mine Web",
    "directories": {
      "output": "dist"
    },
    "files": [
      "node_modules/",
      "frontend/",
      "app.js"
    ],
    "win": {
      "icon": "frontend/img/icon.png",
      "target": "NSIS"
    },
    "extends": null,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "HellLonely",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "flowbite": "^1.8.0",
    "fs-extra": "^11.1.1",
    "opn": "^6.0.0"
  },
  "devDependencies": {
    "electron": "^13.6.9"
  }
}
